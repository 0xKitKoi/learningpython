import mouse

def clicK(x, y):
    startTime = time.time()
    mouse.move(x, y, absolute=True)
    mouse.click()
    print("time taken {}".format(time.time() - startTime))

while keyboard.is_pressed('q') == False:
	this = random.randrange(400, 600)
	that = random.randrange(400, 600)
	clicK(this, that)
